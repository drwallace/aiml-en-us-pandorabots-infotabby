<!--MATERIAL TYPE ATTRIBUTES-->

<topic name="ADBK ATTRIBUTES">  
<category>
<pattern>ATTRADBKS</pattern>
<template>
<think>
<set name="itype_cap">Audiobooks</set>
<set name="itype">audiobooks</set>
<set name="co limit">20</set>
<set name="lper">two weeks</set>
<set name="fine">$0.10 per day</set>
<set name="rlimit">four</set>
<set name="material type attr">ADBK ATTRIBUTES</set>
<set name="gen">GEN</set>
<set name="topic"></set>
</think>
<srai>MTSORT</srai>
</template>
</category>
</topic>

