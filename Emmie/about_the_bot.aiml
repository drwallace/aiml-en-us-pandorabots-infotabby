<li value="AWARD"><set name="message">polaris</set></li>
<li value="* AWARD"><set name="message">polaris</set></li>
<li value="AWARD *"><set name="message">polaris</set></li>
<li value="* AWARD *"><set name="message">aboutaccts</set></li>

<li value="AWARDS"><set name="message">polaris</set></li>
<li value="* AWARDS"><set name="message">polaris</set></li>
<li value="AWARDS *"><set name="message">polaris</set></li>
<li value="* AWARDS *"><set name="message">polaris</set></li>

<li value="ALA"><set name="message">ala2011</set></li>
<li value="* ALA"><set name="message">ala2011</set></li>
<li value="ALA *"><set name="message">ala2011</set></li>
<li value="* ALA *"><set name="message">ala2011</set></li>

