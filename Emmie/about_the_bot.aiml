<li value="* ALLERGIC *"><set name="message">allergy</set></li>

<li value="AIML"><set name="message">coding</set></li>
<li value="* AIML"><set name="message">coding</set></li>
<li value="AIML *"><set name="message">coding</set></li>
<li value="* AIML *"><set name="message">coding</set></li>

<li value="FLASH"><set name="message">coding</set></li>
<li value="* FLASH"><set name="message">coding</set></li>
<li value="FLASH *"><set name="message">coding</set></li>
<li value="* FLASH *"><set name="message">coding</set></li>

<li value="VOICE"><set name="message">voice</set></li>
