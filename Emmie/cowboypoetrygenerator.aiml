<topic name="CWBYPTY">
<category>
<pattern>_</pattern>
<template>
<think>
<set name="answr"><star/></set>
<set name="message">continue</set>
<condition name="answr">

<li value="CEASE"><set name="message">stop</set></li>
<li value="* CEASE"><set name="message">stop</set></li>
<li value="CEASE *"><set name="message">stop</set></li>
<li value="* CEASE *"><set name="message">stop</set></li>

<li value="DESIST"><set name="message">stop</set></li>
<li value="* DESIST"><set name="message">stop</set></li>
<li value="DESIST *"><set name="message">stop</set></li>
<li value="* DESIST *"><set name="message">stop</set></li>

<li value="END"><set name="message">stop</set></li>
<li value="* END"><set name="message">stop</set></li>
<li value="END *"><set name="message">stop</set></li>
<li value="* END *"><set name="message">stop</set></li>

<li value="ENOUGH"><set name="message">stop</set></li>
<li value="* ENOUGH"><set name="message">stop</set></li>
<li value="ENOUGH *"><set name="message">stop</set></li>
<li value="* ENOUGH *"><set name="message">stop</set></li>

<li value="HALT"><set name="message">stop</set></li>
<li value="* HALT"><set name="message">stop</set></li>
<li value="HALT *"><set name="message">stop</set></li>
<li value="* HALT *"><set name="message">stop</set></li>

<li value="NO"><set name="message">stop</set></li>
<li value="* NO"><set name="message">stop</set></li>
<li value="NO *"><set name="message">stop</set></li>
<li value="* NO *"><set name="message">stop</set></li>

<li value="STOP"><set name="message">stop</set></li>
<li value="* STOP"><set name="message">stop</set></li>
<li value="STOP *"><set name="message">stop</set></li>
<li value="* STOP *"><set name="message">stop</set></li>
</condition>
<think><set name="topic"></set></think>
</think>

<condition name="message">
<li value="continue"><srai>CWBYPOEM</srai></li>
<li value="halt"><srai>ENDCBPOETRY</srai></li>

</condition>
</template>
</category>
</topic>

<category>
<pattern>CWBYPOEM</pattern>
<template>OK. Here's one of my poems:<br/><p/>
<srai>CPLN1</srai><br/>
<srai>CPLN2</srai><br/>
